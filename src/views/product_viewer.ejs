					<li class="breadcrumb-item active"><a href="javascript:void(0)">Manage Client</a></li>
				</ol>
				<a class="text-primary fs-13" data-bs-toggle="offcanvas" href="#offcanvasExample1" role="button" aria-controls="offcanvasExample1">+ Add Task</a>
			</div>
			<div class="container-fluid">
				<div class="row">
					<div class="col-xl-12">
						<div class="card">
							<div class="card-body p-0">
								<div class="table-responsive active-projects manage-client">
								<div class="tbl-caption">
									<h4 class="heading mb-0">Manage Client</h4>
								</div>
								
									<table id="reports-tbl" class="table" >
										<thead>
											<tr>
												<th>Product Name</th>
												<th>Product Subname</th>
												<th>Category</th>
												<th>MRP (rs)</th>
												<th>Price (rs)</th>
												<th>Discount (%)</th>
												<th>Quantity</th>
												<th>Action</th>
											</tr>
										</thead>
										<%
										for(let i=0;i<product_data.length;i++){
											%>
											<tbody>
												<tr>
													<td>
														<div class="products">
															<img src="../../public/assets/images/books.png" class="avatar avatar-md" alt="">
															<div>
																<h6><%= product_data[i].name %></h6>
																	
															</div>	
														</div>
													</td>
													
												<!-- </tr> -->
												
												
												<td><span class="text-primary"><%= product_data[i].subname %></span></td>
												<td><span><%= product_data[i].category %></span></td>
												<td><span><%= product_data[i].mrp %></span></td>
												<td><span><%= product_data[i].price %></span></td>	
												<td><span><%= product_data[i].discount %></span></td>	
												<td><span class="badge badge-success light border-0"><%= product_data[i].quantity %></span></td>
												<% _id = product_data[i]._id.toString(); %>

												<td>

												<input type="hidden" value="<%=_id%>" name="_id">

                                                    <button><a href="product_update?id=<%=product_data[i]._id.toString()%>">Update</a></button>

                                                    <form action="product_updated" method="post">													
                                                    <button  type="submit" value="Block" name="answer">Block</button>
													</td>
											</tr>
											
										</form>
											
										</tbody>
										<%
									}
									%>	
									</table>
								
								</div>
							</div>
						</div>
					</div>

    <div id="main-wrapper">
      <!-- Nav header start -->
      <%- include("../../utils/navheader") %>
      <!-- Nav header end -->

      <!-- Chat box start -->
      <%- include("../../utils/chatbox") %>
      <!-- Chat box End -->

      <!-- Header start -->

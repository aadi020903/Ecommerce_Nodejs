      <!-- Header end ti-comment-alt -->

      <!-- Sidebar start -->
      <%- include("../../utils/sidebar") %>
      <!-- Sidebar end -->

      <!--**********************************
            Content body start
        ***********************************-->
      <div class="content-body">
        <!-- row -->
        <div class="container-fluid">
          <!-- row -->
          <div class="row row_1">
            <div class="col-xl-9 col-lg-8">
              <div class="card profile-card card-bx m-b30">
                <div class="card-header">
                  <h6 class="title">Product Details :</h6>
                </div>
                <form
                  class="profile-form"
                  action="/product_update_post"
                  method="post"
                  enctype="multipart/form-data"
                >
                  <input type="hidden" name="FolderName" value="products" />
                  <div class="card-body">
                    <div class="row">
                      <% let id = data._id.toString() %>
                      <input type="hidden" name="id" value="<%= id %>" />

                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Product Name</label>
                        <input
                          type="text"
                          class="form-control"
                          name="name"
                          value="<%= data.name %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Subname</label>
                        <input
                          type="text"
                          class="form-control"
                          name="subname"
                          value="<%= data.subname %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Brand</label>
                        <input
                          type="text"
                          class="form-control"
                          name="brand"
                          value="<%= data.brand %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Category </label>
                        <select
                          class="default-select form-control categorySelect"
                          id="validationCustom05"
                          name="category"
                        >
                          <option data-display="Select">Please select</option>
                          <% category_data.forEach(element=> { %>
                          <option><%= element.category_name %></option>
                          <%}); %>
                        </select>
                      </div>

                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Sub-Category</label>
                        <select
                          class="default-select subCategorySelect form-control"
                          id="validationCustom05"
                          name="sub_category"
                        >
                          <option class="" data-display="Select">
                            Please select
                          </option>
                        </select>
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Delivery Category</label>
                        <select
                          class="default-select form-control"
                          id="validationCustom05"
                          name="delivery_category"
                          value="<%= data.delivery_category %>"
                        >
                          <option data-display="Select">Please select</option>
                          <option value="light">Light</option>
                          <option value="medium">Medium</option>
                          <option value="heavy">Heavy</option>
                        </select>
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">M.R.P ₹</label>
                        <input
                          type="number"
                          class="form-control"
                          name="mrp"
                          value="<%= data.mrp %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Price ₹</label>
                        <input
                          type="number"
                          class="form-control"
                          name="price"
                          value="<%= data.price %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Discount %</label>
                        <input
                          type="number"
                          class="form-control"
                          name="discount"
                          value="<%= data.discount %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Quantity</label>
                        <input
                          type="number"
                          class="form-control"
                          name="quantity"
                          value="<%= data.quantity %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Size</label>
                        <input
                          type="text"
                          class="form-control"
                          name="size"
                          value="<%= data.size %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Color</label>
                        <input
                          type="text"
                          class="form-control"
                          name="color"
                          value="<%= data.color %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Description</label>
                        <input
                          type="text"
                          class="form-control"
                          name="description"
                          value="<%= data.description %>"
                        />
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Photos ( upto 4 )</label>
                        <input
                          type="file"
                          class="form-control"
                          name="photos"
                          multiple
                        />
                      </div>
                    </div>
                  </div>
                  <div class="card-footer">
                    <button class="btn btn-primary">ADD PRODUCT</button>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
